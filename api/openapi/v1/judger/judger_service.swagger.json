        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/judgerRunCodeRequest"
            }
          }
        ],
        "tags": [
          "Code"
        ]
      }
    }
  },
  "definitions": {
