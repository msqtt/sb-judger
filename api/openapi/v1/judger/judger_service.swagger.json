            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/judgerJudgeCodeRequest"
            }
          }
        ],
        "tags": [
          "Code"
        ]
      }
    },
